{
  "name": "hackpsus2018",
  "version": "0.0.1",
  "description": "Frontend repository for hackPSU",
  "scripts": {
    "lint": "eslint ./website/js/main.js --ignore node_modules --cache --ext .js --fix && travis lint .travis.yml && styleline **/*.scss",
    "test": "echo 'success'",
    "watch-sass": "sass --watch ./website/scss/main.scss:./website/css/main-compiled.css ./website/scss/night-main.scss:./website/css/night-main-compiled.css ./website/scss/day-main.scss:./website/css/day-main-compiled.css ./website/scss/business.scss:./website/css/business-compiled.css",
    "sass": "sass ./website/scss/main.scss:./website/css/main-compiled.css ./website/scss/night-main.scss:./website/css/night-main-compiled.css ./website/scss/day-main.scss:./website/css/day-main-compiled.css ./website/scss/business.scss:./website/css/business-compiled.css",
    "start": "npm run sass"
  },
  "keywords": [
    "hackPSU"
  ],
  "author": "hackpsu-tech",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "stylelint": "9.4.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-scss": "3.2.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "sass": "^1.26.7"
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": "stylelint-config-recommended"
  }
}
