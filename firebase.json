{
  "hosting": [{
    "target": "website",
    "public": "website",
    "ignore": ["firebase.json", "**/.*", "**/scss/**", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  {
    "target": "app",
    "public": "user-registration-app/dist",
    "ignore": ["firebase.json", "**/.*", "**/scss/**", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }]
}
